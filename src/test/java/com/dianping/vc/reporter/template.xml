<?xml version="1.0" encoding="utf-8" ?>
<excel>
    <sheet name="sheetCeshi">
        <row>
            <cell>
               <text value="序号"></text>
            </cell>
            <cell>
                <text value="姓名"></text>
            </cell>
            <cell>
                <text value="年龄"></text>
            </cell>
            <cell>
                <text value="性别"></text>
            </cell>
        </row>
        <row repeat="List">
            <cell>
                <style>
                    <font color="red"/>
                </style>
                <text value="$_listPhysicalIndex"></text>
            </cell>
            <cell>
                <text value="{name}"></text>
            </cell>
            <cell>
                <text value="{age}"></text>
            </cell>
            <cell>
                <text value="{sex}"></text>
            </cell>
        </row>
    </sheet>
</excel>